{
  "expo": {
    "name": "wordfind",
    "slug": "wordfind",
    "sdkVersion": "52.0.0",
    "version": "1.0.6",
    "jsEngine": "hermes",
    "orientation": "portrait",
    "platforms": ["ios", "android", "web"],
    "icon": "./assets/icon.png",
    "scheme": "wordfind",
    "runtimeVersion": "1.0.0",
    "updates": {
      "enabled": true,
      "checkAutomatically": "ON_LOAD",
      "fallbackToCacheTimeout": 0,
      "url": "https://u.expo.dev/ec090143-64a0-4f38-8383-5c85886c1e70"
    },
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "cover",
      "backgroundColor": "#ffffff"
    },
    "owner": "ayiyazilim",
    "userInterfaceStyle": "automatic",
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "buildNumber": "1.0.6",
      "supportsTablet": true,
      "bundleIdentifier": "com.ayiyazilim.wordfind",
      "infoPlist": {
        "NSMicrophoneUsageDescription": "This app does not require access to the microphone.",
        "ITSAppUsesNonExemptEncryption": false
      }
    },
    "android": {
      "versionCode": 6,
      "adaptiveIcon": {
        "foregroundImage": "./assets/icon.png",
        "backgroundColor": "#000"
      },
      "package": "com.ayiyazilim.wordfind",
      "permissions": ["INTERNET", "android.permission.MODIFY_AUDIO_SETTINGS"]
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "name": "wordfind",
      "shortName": "wordfind",
      "favicon": "./assets/favicon.png"
    },
    "extra": {
      "eas": {
        "projectId": "ec090143-64a0-4f38-8383-5c85886c1e70"
      }
    },
    "plugins": ["expo-asset"]
  }
}

{
  "expo": {
    "name": "wordfind",
    "slug": "wordfind",
    "sdkVersion": "52.0.0",
    "version": "2.0.2",
    "jsEngine": "hermes",
    "orientation": "portrait",
    "platforms": ["ios", "android", "web"],
    "icon": "./assets/icon.png",
    "scheme": "wordfind",
    "runtimeVersion": "2.0.2",
    "updates": {
      "enabled": true,
      "checkAutomatically": "ON_LOAD",
      "fallbackToCacheTimeout": 0,
      "url": "https://u.expo.dev/ec090143-64a0-4f38-8383-5c85886c1e70"
    },
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "cover",
      "backgroundColor": "#ffffff"
    },
    "owner": "ayiyazilim",
    "userInterfaceStyle": "automatic",
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "buildNumber": "2.0.2",
      "supportsTablet": true,
      "bundleIdentifier": "com.ayiyazilim.wordfind",
      "infoPlist": {
        "NSMicrophoneUsageDescription": "This app does not require access to the microphone.",
        "ITSAppUsesNonExemptEncryption": false
      }
    },
    "android": {
      "versionCode": 6,
      "adaptiveIcon": {
        "foregroundImage": "./assets/icon.png",
        "backgroundColor": "#000"
      },
      "package": "com.ayiyazilim.wordfind",
      "permissions": ["INTERNET", "android.permission.MODIFY_AUDIO_SETTINGS"]
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "name": "wordfind",
      "shortName": "wordfind",
      "favicon": "./assets/favicon.png"
    },
    "extra": {
      "eas": {
        "projectId": "ec090143-64a0-4f38-8383-5c85886c1e70"
      }
    },
    "plugins": [
      "expo-asset",
      [
        "expo-build-properties",
        {
          "ios": {
            "useFrameworks": "static"
          }
        }
      ],
      [
        "expo-av",
        {
          "microphonePermission": false
        }
      ],
      [
        "react-native-google-mobile-ads",
        {
          "androidAppId": "ca-app-pub-3105662444743172~9118850389",
          "iosAppId": "ca-app-pub-3105662444743172~8927278696",
          "userTrackingDescription": "This identifier will be used to deliver personalized ads to you."
        }
      ]
    ]
  }
}
